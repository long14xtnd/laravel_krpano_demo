			onout="copy(bgcolor, bgcolor_t); copy(bgalpha, bgalpha_t);"
			onclick.addevent="click_butt();"
			ondown="ondown_butt();"
			onloaded.addevent="txtreplace(b_id, name,'line_',); calc(txt_m, 'txt_' + b_id); "			
		/>
		

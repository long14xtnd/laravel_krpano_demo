    <style  name="container_thumbnails" type="container" keep="true" align="top"  maskchildren="true"  itemm="true" b_id="" txt_m="" group="" edit_group="true" bgcolor_t="" pos=""				onclick_temp="" edit_all_group="true" bg="true"
